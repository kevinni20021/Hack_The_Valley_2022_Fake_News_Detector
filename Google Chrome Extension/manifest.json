{
	"manifest_version": 3,
	"name": "Credable",
	"description": "Creating a more credible world",
	"version": "1.0",
	"permissions": [
		"tabs"
	],
	"host_permissions" : [
		"http://127.0.0.1:5000/scrape",
		"https://*/",
		"http://*/"
	],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "action.html",
		"default_icon": {
			"16" : "16_fake_new_logo.png",
			"32" : "32_fake_new_logo.png",
			"128" : "128_fake_new_logo.png"
		}
	}

}